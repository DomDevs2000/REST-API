{
  "compilerOptions": {
    "strict": true,
    "target": "ES2022",
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "allowJs": true,
    "outDir": "dist",
    "module": "es6",
    "moduleResolution": "node",
    "lib": [
      "ES2022"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "preserve"
  }
}
